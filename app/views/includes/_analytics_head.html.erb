<% unless @google_tag_manager.empty? %>
  <%= javascript_include_tag 'transformation/dit.tagging' %>
  <% @google_tag_manager.each_with_index do |id, index| %>
    <!-- Google Tag Manager (<%= index %>) -->
    <script>(function(w,d,s,l,i){w[l]=w[l]||[];w[l].push({'gtm.start':
    new Date().getTime(),event:'gtm.js'});var f=d.getElementsByTagName(s)[0],
    j=d.createElement(s),dl=l!='dataLayer'?'&l='+l:'';j.async=true;j.src=
    'https://www.googletagmanager.com/gtm.js?id='+i+dl+ '&gtm_auth=<%= Figaro.env.GOOGLE_TAG_MANAGER_AUTH %>&gtm_preview=<%= Figaro.env.GOOGLE_TAG_MANAGER_ENVIRONMENT %>&gtm_cookies_win=x';f.parentNode.insertBefore(j,f);
    })(window,document,'script','dataLayer','<%= id %>');</script>
    <!-- End Google Tag Manager (<%= index %>) -->
  <% end %>
<% end %>
