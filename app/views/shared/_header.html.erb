<%# This partial decides which header to render based on the bgs_site? method %>
<% header_locals = { sso_is_logged_in: user_signed_in_cookie? } %>

<% if bgs_site? %>
  <%= render partial: 'shared/header_bgs', locals: header_locals %>
<% else %>
  <%= render partial: 'shared/header_great', locals: header_locals %>
<% end %>
