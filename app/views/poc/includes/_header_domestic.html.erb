<!-- For POC - have cleaned up files autogenerated by script in great-pattern-library. -->

<header id="header">
  <a id="skip-link" href="#content" class="verbose">Skip to main content</a>
  <%= render partial: "components/language_selector" %>

  <div id="personal-bar">
    <div class="container">
      <ul class="navigation account-links" aria-label="Account links">
      <% if !current_user %>
        <li class="anonymous"><a href="<%=Figaro.env.SSO_ENDPOINT_BASE_URI %>accounts/signup" class="register">Register</a></li>
        <li class="anonymous"><a href="<%=dashboard_path%>" class="signin">Sign in</a></li>
      <% else %>
        <li class="authenticated"><a href="<%=dashboard_path%>" class="profile">Profile</a></li>
        <li class="authenticated"><a href="<%=destroy_user_session_path%>" class="signout">Sign out</a></li>
      <% end %>
      </ul>
    </div>
  </div>

  <div class="container">
    <a id="header-logo" href="<%=Figaro.env.GREAT_GOV_URL %>">
      <img src="<%=asset_path('logos/DfIT_WHITE_AW.png')%>" alt="Home">
    </a>
  </div>

  <nav id="menu">
    <div class="container">
      <ul class="navigation level-1">
        <li>
          <a href="<%=Figaro.env.GREAT_GOV_URL %>">Home</a>
        </li>
        <li>
          <a href="<%=Figaro.env.GREAT_GOV_URL %>/custom">Your export journey</a>
        </li>
        <li>
          <a href="#export-readiness-links-list" id="export-readiness-links" class="list-header">Export readiness</a>
          <ul id="export-readiness-links-list" aria-labelledby="export-readiness-links" class="navigation level-2">
              <li class="items-3"><a id="header-export-readiness-new" href="<%=Figaro.env.GREAT_GOV_URL %>/new">I&#39;m new to exporting</a></li>
              <li class="items-3"><a id="header-export-readiness-occasional" href="<%=Figaro.env.GREAT_GOV_URL %>/occasional">I export occasionally</a></li>
              <li class="items-3"><a id="header-export-readiness-regular" href="<%=Figaro.env.GREAT_GOV_URL %>/regular">I&#39;m a regular exporter</a></li>
            </ul>
          </li>
          <li><a href="#guidance-links-list" id="header-guidance-links" class="list-header">Guidance</a>
            <ul id="guidance-links-list" aria-labelledby="header-guidance-links" class="navigation level-2">
              <li class="items-6"><a id="header-guidance-market-research" href="<%=Figaro.env.GREAT_GOV_URL %>/market-research">Market research</a></li>
              <li class="items-6"><a id="header-guidance-customer-insight" href="<%=Figaro.env.GREAT_GOV_URL %>/customer-insight">Customer insight</a></li>
              <li class="items-6"><a id="header-guidance-finance" href="<%=Figaro.env.GREAT_GOV_URL %>/finance">Finance</a></li>
              <li class="items-6"><a id="header-guidance-business-planning" href="<%=Figaro.env.GREAT_GOV_URL %>/business-planning">Business planning</a></li>
              <li class="items-6"><a id="header-guidance-getting-paid" href="<%=Figaro.env.GREAT_GOV_URL %>/getting-paid">Getting paid</a></li>
              <li class="items-6"><a id="header-guidance-operations-and-compliance" href="<%=Figaro.env.GREAT_GOV_URL %>/operations-and-compliance">Operations and compliance</a></li>
            </ul>
          </li>
          <li><a href="#services-links-list" id="header-services-links" class="list-header">Services</a>
            <ul id="services-links-list" aria-labelledby="header-services-links" class="navigation level-2">
              <li class="items-5">
                <a id="header-services-find-a-buyer" href="<%=Figaro.env.FIND_A_BUYER_URL %>">Create an export profile</a><span class="description">Get promoted internationally with a great.gov.uk trade profile</span>
              </li>
              <li class="items-5">
                <a id="header-services-selling-online-overseas" href="<%=Figaro.env.SELLING_ONLINE_OVERSEAS_URL %>">Sell online overseas</a><span class="description">Find the right marketplace for your business and access special offers for sellers</span>
              </li>
              <li class="items-5">
                <a id="header-services-export-opportunities" href="<%=Figaro.env.EXPORT_OPPORTUNITIES_URL %>">Find export opportunities</a><span class="description">Find and apply for overseas opportunities</span>
              </li>
              <li class="items-5">
                <a id="header-services-get-finance" href="<%=Figaro.env.GREAT_GOV_URL %>/get-finance">Get finance</a><span class="description">Get the finance you need to compete and grow</span>
              </li>
              <li class="items-5">
                <a id="header-services-events" href="<%=Figaro.env.EVENTS_URL %>">Find events and visits</a><span class="description">Attend events and see how visits by ministers can support your trade deals</span>
              </li>
            </ul>
          </li>
        </ul>
      </nav>
    </div>
  </div>
</header>
